				modulo="APP_ORDERS";
				$.each(data.body.authorities.APP_ORDERS, function() {
	
						querys[y]='INSERT OR IGNORE INTO security (username, module , action )  VALUES ("'+ localStorage['usuario']+'", "'+modulo+'", "'+this+'")';
